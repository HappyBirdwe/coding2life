/* pages/changeStyle/index.wxss */
.container{
  min-height: 100%;
  background-color:#F2F2F2;
  overflow: hidden;
  padding-top: 20rpx;
}
.tab{
  padding:0 40rpx;
  height: 90rpx;
  line-height: 90rpx;
  color: #333;
  position: relative;
  background-color: #fff;
  
}
.tab:nth-child(1){
  border-bottom: 1rpx solid #dedede;
}
.tab image{
  position: absolute;
  width: 56rpx;
  height: 40rpx;
  right:42rpx;
  top: 25rpx;
}

/*  footer  */
.btn{
  width: 670rpx;
  height: 88rpx;
  line-height: 88rpx;
  text-align: center;
  border-radius: 10rpx;
  -webkit-border-radius: 10rpx;
  margin: 50rpx auto 0;
  font-size: 32rpx;
}
