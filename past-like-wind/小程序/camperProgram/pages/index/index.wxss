.rent_banner{
  width: 670rpx;
  margin: 50rpx auto;
  height: 336rpx;
  position: relative;
}
.rent_banner image{
  width: 100%;
  height: 100%;
}

.day {
    width:52rpx;
    padding-right: 8rpx;
}

.auth_dialog{
  width: 560rpx;
  height: 600rpx;
  position: fixed;
  left: 95rpx;
  bottom: 300rpx;
  background-color: #fff;
  z-index:99;
  border-radius: 6rpx;
  animation:dialogShow 0.3s 0s ease-in;
}
.auth_dialog .auth_title{
  color: #111;
  font-size: 38rpx;
  text-align: center;
  padding: 50rpx 0;
  position: relative;
}
.auth_title image{
  width: 24rpx;
  height: 24rpx;
  position: absolute;
  right: 20rpx;
  top:20rpx;
  z-index: 12;
}
.auth_main{
  text-align: center;
  height: 400rpx;
}
.auth_main image{
  width:279rpx;
  display: inline-block;
  height: 201rpx;
}
.auth_main button{
  background-color: #f8b62b;
  width: 460rpx;
  display: inline-block;
  height: 88rpx;
  border-radius: 60rpx;
  color: #fff;
  z-index: 12;
  border:none;
  margin-top: 70rpx;
  letter-spacing: 2rpx;
}
.sign_dialog{
   height: 520rpx;
}
.sign_main{ text-align: center;height: 300rpx;}
.sign_suptitle{color: #000;font-size: 34rpx;}
.sign_subtitle{color: #999;font-size: 30rpx;margin-top: 30rpx;margin-bottom: 60rpx;}
.sign_btn{
  width: 500rpx;
  height: 68rpx;
  text-align: center;
  line-height: 66rpx;
  font-size: 32rpx;
  margin: 20rpx auto 0;
  border-radius: 8rpx;
}
.sign_btn_primary{
  background-color: #F8B62B;
  color: #fff;
  border:1rpx solid #f8b62b;
}
.sign_btn_primary text{ display: inline-block;font-size: 24rpx;margin-left: 30rpx; }
.sign_btn_default{border:1rpx solid #e1e1e1; color: #333;}

@keyframes dialogShow {
  0%   {transform: scale(0);}
  100% {transform: scale(1);}
}

@keyframes cover {
  0%   {background-color: rgba(0,0,0,0.3);}
  100% {background-color: rgba(0,0,0,0.5);}
}

.form_btn{
  position: absolute;
  left: 0;
  top:0;
  width: 100%;
  height: 100%;
  z-index:10;
}
.form_btn button{
  width: 100%;
  height: 100%;
  background-color: transparent;
  border:none;
}